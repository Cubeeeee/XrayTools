{
  "NacosHost": "http://",
  "NacosApis": [
    {
      "Name": "GetConfig",
      "Description": "获取Nacos上的配置",
      "Method": "GET",
      "Url": "/nacos/v1/cs/configs/listener",
      "Parms": "tenant={0}&dataId={1}&group={2}",
      "Body": ""
    },
    {
      "Name": "ListenConfig",
      "Description": "监听配置",
      "Method": "POST",
      "Url": "/nacos/v1/cs/configs",
      "Parms": "tenant={0}&dataId={1}&group={2}",
      "Body": "Listening-Configs={0}^2{1}^2{2}^2{3}^1"
    },
    {
      "Name": "RegInstance",
      "Description": "注册一个实例到服务。",
      "Method": "POST",
      "Url": "/nacos/v1/ns/instance",
      "Parms": "ip={0}&port={1}&namespaceId={2}&serviceName={3}&clusterName={4}&weight={5}",
      "Body": ""
    },
    {
      "Name": "Beat",
      "Description": "发送某个实例的心跳",
      "Method": "PUT",
      "Url": "/nacos/v1/ns/instance/beat",
      "Parms": "serviceName={0}&beat={1}",
      "Body": ""
    },
    {
      "Name": "CreateService",
      "Description": "创建一个服务",
      "Method": "POST",
      "Url": "/nacos/v1/ns/service",
      "Parms": "serviceName={0}&namespaceId={1}&healthCheckMode=client",
      "Body": ""
    },
    {
      "Name": "QueryService",
      "Description": "查询一个服务",
      "Method": "GET",
      "Url": "/nacos/v1/ns/service",
      "Parms": "serviceName={0}&namespaceId={1}",
      "Body": ""
    },
    {
      "Name": "QueryServiceList",
      "Description": "查询服务列表",
      "Method": "GET",
      "Url": "/nacos/v1/ns/service/list",
      "Parms": "pageNo=1&pageSize=1000&namespaceId={0}",
      "Body": ""
    }
  ],
  "Route": {
    "address": "http://host/dynagateway/save",
    "routeinfo": {
      "filters": [
        {
          "args": {
            "parts": "1"
          },
          "name": "StripPrefix"
        }
      ],
      "id": "{ServiceName}",
      "order": 0,
      "predicates": [
        {
          "args": {
            "pattern": "/{ServiceName}/**"
          },
          "name": "Path"
        }
      ],
      "uri": "lb://{ServiceName}/**"
    }

  }
}
